                                @Html.TextBox("AS_DATE", null, new { @class = "form-control datepicker", @autocomplete = "off" })
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.Label("AS_DATE", "วันที่ต้องการ : ", new { @class = "control-label col-md-4" })

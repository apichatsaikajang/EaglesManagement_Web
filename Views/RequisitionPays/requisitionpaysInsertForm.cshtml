                                <div class="radio">
                                    <label>
                                        @Html.RadioButton("CUS_PART", 1, true, new { @onchange = "chgComPart()" }) ขอเบิก
                                    </label>
                                    &nbsp;&nbsp;&nbsp;
                                    <label>
                                        @Html.RadioButton("CUS_PART", 2, new { @onchange = "chgComPart()" }) ขอซื้อ
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.Label("", " ", new { @class = "control-label col-md-4" })
                            <div class="col-md-4">

                                <div class="form-inline">
                                    @Html.CheckBox("อนุมัต/รับทราบ", false)
                                    <label for="Field1">อนุมัต/รับทราบ</label>
                                </div>

                                <div class="form-inline">
                                    @Html.CheckBox("ส่งของแล้ว", false)
                                    <label for="Field1">ส่งของแล้ว</label>
                                </div>

                                <div class="form-inline">
                                    @Html.CheckBox("ส่งต่อให้พี่วัลพิจารณาอนุมัติโดยจัดซื้อ", false)
                                    <label for="Field1"> ส่งต่อให้พี่วัลพิจารณาอนุมัติโดยจัดซื้อ</label>
                                </div>

                                <div class="form-inline">
                                    @Html.CheckBox("พี่วัลอนุมัตจัดซื้อ", false)
                                    <label for="Field1">พี่วัลอนุมัตจัดซื้อ</label>
                                </div>

                                <div class="form-inline">
                                    @Html.CheckBox("กำลังซื้อของ", false)
                                    <label for="Field1">กำลังซื้อของ</label>
                                </div>
                            </div>
                        </div>






                        <div class="form-group">
                            @Html.Label("MILE_IN", "เลขที่เอกสาร : ", new { @class = "control-label col-md-4" })
                            <div class="col-md-5">
                                @Html.TextBox("MILE_IN", null, new { @class = "form-control" })
                            </div>
                        </div>

                        <div class="form-group">
                            @Html.Label("AS_DATE", "วันที่ ขอเบิก/ซื้อ : ", new { @class = "control-label col-md-4" })

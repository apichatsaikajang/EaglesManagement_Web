}

@section custom_script{
    <script>
        $(document).ready(function () {
            //Date picker
            $('.datepicker').datepicker({
                autoclose: true,
                //todayBtn: true,
                todayHighlight: true,
                format: 'dd-mm-yyyy',
                startDate: Date()
            });
        });
    </script>

    <script>
        $(document).ready(function () {
            //Timepicker
            $('.timepicker').timepicker({
                showInputs: false,
                defaultTime: 'current',
                showMeridian: false,
                minuteStep: 1,
            });
        });
    </script>



}

@section header{
    <h1>
        RequisitionPays
        <small>Insert Form</small>
    </h1>
}

@section content{
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header">
                    <div class="col-md-6" style="padding:initial;">
                        <h3></h3>
                    </div>
                    <div class="col-md-6 text-right" style="padding:initial;">
                        <a href="@Url.Action("Index", "RequisitionPays")" class="btn btn-primary"><i class="fa fa-reply"></i>&nbsp;&nbsp;Back</a>
                    </div>
                </div>
                <div class="box-body">

                    @using (Html.BeginForm("requisitionpaysInsertForm", "RequisitionPays", FormMethod.Post, new { @class = "form-horizontal" }))
                    {


                        <div class="form-group">
                            @Html.Label("CUSTOMER", "กรุณาเลือกระเภท : ", new { @class = "control-label col-md-4" })
